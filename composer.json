{
    "name": "spyrit/billetel-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for Billetel client",
    "homepage": "http://www.spyrit.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Spyrit",
            "email": "support@spyrit.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0",
        "spyrit/billetel": "dev-symfony4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "^6.2",
        "matthiasnoback/symfony-config-test": "^4.3",
        "symfony/yaml": "^4.4|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spyrit\\BilletelBundle\\":""
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spyrit\\BilletelBundle\\Tests\\": "Tests/"
        }
    }
}
